{
	"scripts": {
		"lint": "tsc --noEmit && eslint ./src",
		"fix": "eslint src --fix",
		"dev": "unbuild --stub && node dist/app.mjs",
		"dev-public": "unbuild --stub && node dist/app.mjs -public-mode",
		"dev-cli": "unbuild --stub && node dist/app.mjs -cli",
		"build": "unbuild",
		"cli": "unbuild && node dist/app.mjs -cli",
		"prebuild": "eslint src",
		"start": "node dist/app.mjs",
		"format": "pnpm run \"/^format:.*/\"",
		"format:prettier": "prettier --write \"*.{js,ts,json,yaml,md,cjs,mjs}\" \"./src/**/*.{js,ts,css,json,ml,yaml,html}\"",
		"prepare": "husky"
	},
	"dependencies": {
		"@larksuiteoapi/node-sdk": "^1.43.0",
		"axios": "^1.8.1",
		"axios-retry": "^4.5.0",
		"chalk": "^5.4.1",
		"chalk-template": "^1.1.0",
		"dotenv": "^16.4.7",
		"moment-timezone": "^0.5.47",
		"mysql2": "^3.12.0",
		"node-cron": "^3.0.3",
		"punycode": "^2.3.1",
		"puppeteer": "^24.3.0",
		"sequelize": "^6.37.5",
		"telegraf": "^4.15.3"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.7",
		"@eslint/js": "^9.23.0",
		"@trivago/prettier-plugin-sort-imports": "^5.2.2",
		"@types/node": "^22.13.8",
		"@types/node-cron": "^3.0.11",
		"@typescript-eslint/parser": "^8.29.0",
		"eslint": "^9.21.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-import-resolver-typescript": "^3.8.3",
		"eslint-plugin-import-x": "^4.6.1",
		"eslint-plugin-prettier": "^5.2.3",
		"globals": "^16.0.0",
		"husky": "^9.1.7",
		"prettier": "^3.5.2",
		"ts-node": "^10.9.2",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.25.0",
		"unbuild": "^3.5.0"
	},
	"packageManager": "pnpm@10.7.1"
}
